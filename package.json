{
  "name": "gen-readlines",
  "version": "1.0.1",
  "description": "Generator based line reader",
  "engines": {
    "node": ">=6"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "tsc --noEmit && mocha --compilers ts:ts-node/register ./test/*.ts",
    "format": "prettier --write {perf,test}/*.{js,ts} index.ts ./*.{ts,js}",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/neurosnap/gen-readlines.git"
  },
  "keywords": [
    "generator",
    "file",
    "line",
    "reader",
    "read",
    "es6",
    "readlines",
    "readline"
  ],
  "author": "Eric Bower",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neurosnap/gen-readlines/issues"
  },
  "homepage": "https://github.com/neurosnap/gen-readlines#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.27",
    "byline": "^4.2.1",
    "husky": ">=4",
    "line-reader": "^0.4.0",
    "linebyline": "^1.3.0",
    "lint-staged": ">=10",
    "mocha": "^2.2.5",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,md}": "prettier --write"
  }
}
